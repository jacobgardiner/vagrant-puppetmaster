#
# Test vhost
#
NameVirtualHost *:80
<VirtualHost *:80>
  ServerName testvhost
  DocumentRoot <%= docroot %>
  <Directory <%= docroot %>>
    Options <%= options %>
    AllowOverride <%= Array(override).join(' ') %>
    Order allow,deny
    allow from all
  </Directory>
  ErrorLog /var/log/httpd/error.log
  LogLevel warn
  CustomLog /var/log/httpd/access.log combined
  ServerSignature On
</VirtualHost>
